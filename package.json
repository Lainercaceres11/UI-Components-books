          "**/*.test.jsx*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off",
          "no-undef": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      "> 0.5%, last 2 versions, Firefox ESR, not dead",
